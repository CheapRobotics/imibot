<robot name="imibot">

  <link name="base_link">
    <inertial>
      <mass value="2"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0282916666667" ixy="0" ixz="0" iyy="0.0282916666667" iyz="0" izz="0.05625"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.255 0.1 0.039"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.032"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.255 0.1 0.039"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.032"/>
    </collision>
  </link>

  <link name="rear_left_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.032" length="0.03"/>
      </geometry>
      <origin rpy="0 1.57079632679 1.57079632679" xyz="0 0 0"/> 
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.032" length="0.03"/>
      </geometry>
      <origin rpy="0 1.57079632679 1.57079632679" xyz="0 0 0"/> 
    </collision>
  </link>

  <link name="rear_right_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.032" length="0.03"/>
      </geometry>
      <origin rpy="0 1.57079632679 1.57079632679" xyz="0 0 0"/> 
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.032" length="0.03"/>
      </geometry>
      <origin rpy="0 1.57079632679 1.57079632679" xyz="0 0 0"/> 
    </collision>
  </link>

  <link name="front_left_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.032" length="0.03"/>
      </geometry>
      <origin rpy="0 1.57079632679 1.57079632679" xyz="0 0 0"/> 
      <material name="black">
        <color rgba="0 0 0 1"/>
